{
  "name": "@pinttosoft/noba_structure",
  "version": "2.2.4-dev41",
  "description": "Libreria de dominio e infraestructura para la estrcutura de cuentas segregadas",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" ",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --forceExit --runInBand --detectOpenHandles --testTimeout=30000",
    "publish-version": "npm run clean && npm i && npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinttosoft/noba_lib_segregated_structure.git"
  },
  "author": "angel bej arano / angel.bejarano@jaspesoft.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinttosoft/noba_lib_segregated_structure/issues"
  },
  "homepage": "https://github.com/pinttosoft/noba_lib_segregated_structure#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/jest": "^29.5.4",
    "@types/uuid": "^9.0.2",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^3.0.2",
    "ts-auto-mock": "^3.7.1",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.0.6",
    "@hubspot/api-client": "^11.1.0",
    "aws-sdk": "^2.1466.0",
    "bcryptjs": "^2.4.3",
    "mongodb": "^6.5.0",
    "rimraf": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "jest": {
    "testTimeout": 40000,
    "setupFiles": [
      "./test/jest/config.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/$1",
      "^test/(.*)": "<rootDir>/../test/$1"
    },
    "testRegex": ".spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "coverage"
        }
      ]
    ]
  }
}
